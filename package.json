  "scripts": {
    "g-co": "nx workspace-generator component",
    "g-page": "nx workspace-generator page",
    "g-page-id": "nx workspace-generator page-id",
    "g-constant": "nx workspace-generator constant",
    "g-service": "nx workspace-generator service",
    "codegen": "graphql-codegen",
    "cypress-install": "cypress install --force",
    "tsc": "nx run-many --target=tsc",
    "dev": "nx run-many --target=serve",
    "build": "nx run-many --target=build",
    "prod": "pnpm build && nx run-many --target=serve --prod",
    "e2e": "nx e2e ui-e2e",
    "e2e-watch": "nx e2e ui-e2e --watch",
    "lint": "nx run-many --target=lint",
    "test": "nx run-many --target=test",
    "prettier-format": "prettier --write . && git add --all",
    "prepare": "husky install",
    "serve-backend": "nx serve backend --prod",
    "serve-ui": "nx serve ui --prod",
    "serve-all": "start-server-and-test serve-backend 3333 serve-ui",
    "build-and-start": "pnpm build && pnpm serve-all"
  },
